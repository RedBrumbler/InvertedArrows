{
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "info": {
    "name": "InvertedArrows",
    "id": "invertedarrows",
    "version": "1.0.9",
    "url": "https://github.com/RedBrumbler/InvertedArrows",
    "additionalData": {
      "overrideSoName": "libinvertedarrows.so"
    }
  },
  "dependencies": [
    {
      "id": "beatsaber-hook",
      "versionRange": "^3.14.0",
      "additionalData": {
        "extraFiles": [
          "src/inline-hook"
        ]
      }
    },
    {
      "id": "codegen",
      "versionRange": "^0.33.0",
      "additionalData": {}
    },
    {
      "id": "custom-types",
      "versionRange": "^0.15.23",
      "additionalData": {}
    },
    {
      "id": "bs-utils",
      "versionRange": "^0.7.1",
      "additionalData": {}
    },
    {
      "id": "bsml",
      "versionRange": "^0.3.1",
      "additionalData": {}
    }
  ],
  "workspace": {
    "scripts": {
      "clean": [
        "pwsh ./scripts/build.ps1 -clean"
      ],
      "log": [
        "pwsh ./scripts/log.ps1"
      ],
      "build": [
        "pwsh ./scripts/build.ps1"
      ],
      "qmod": [
        "pwsh ./scripts/build.ps1 -clean",
        "qpm-rust qmod build",
        "pwsh ./scripts/createqmod.ps1 InvertedArrows -clean"
      ],
      "copy": [
        "pwsh ./scripts/copy.ps1"
      ]
    }
  }
}